
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model Role {
id String @id @default(uuid())
name String
}

model User {
  id String @id @default(uuid())
  firstname String
  lastname String
  email String @unique
  password String
  biography String
  role_id String
  referal_code String
  avatar String

  
  

}
        